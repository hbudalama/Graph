
  query ($userId: Int!) {
    transaction(where: { 
      userId: { _eq: $userId }
      type: { _like: "skill_%" } 
    }) {
      id
      type
      amount
      path
      createdAt
      attrs
      originEventId
      originEvent { 
        path 
        object { type } 
      }
      object { name }
    }
  }
